{
    "name": "sebastienheyd/boilerplate-email-editor",
    "description": "E-mail visual editor for sebastienheyd/boilerplate",
    "keywords": ["laravel", "boilerplate", "email", "mail", "e-mail", "editor"],
    "type": "library",
    "require": {
        "sebastienheyd/boilerplate": "^5.7.3",
        "sebastienheyd/boilerplate-media-manager": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.0",
        "orchestra/testbench": "^3.8",
        "doctrine/dbal": "^2.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastien HEYD",
            "email": "contact@sheyd.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sebastienheyd\\BoilerplateEmailEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sebastienheyd\\BoilerplateEmailEditor\\BoilerplateEmailEditorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
