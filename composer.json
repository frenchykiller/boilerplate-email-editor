{
    "name": "sebastienheyd/boilerplate-email-editor",
    "description": "E-mail visual editor for sebastienheyd/boilerplate",
    "keywords": ["laravel", "boilerplate", "email", "mail", "e-mail", "editor"],
    "type": "library",
    "require": {
        "sebastienheyd/boilerplate": "^6.1|dev-master",
        "sebastienheyd/boilerplate-media-manager": "^6.1|dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "orchestra/testbench": "^4.0",
        "doctrine/dbal": "^2.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastien HEYD",
            "email": "contact@sheyd.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sebastienheyd\\BoilerplateEmailEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sebastienheyd\\BoilerplateEmailEditor\\BoilerplateEmailEditorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
